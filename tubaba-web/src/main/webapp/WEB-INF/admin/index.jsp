	 var admin;
	$.ajax({
		url:'queryAdmin',
		dataType:"json",
		type:"Get",
		async:false,//默认为异步请求，设置false为同步
		success: function(res){
			admin = res;
		}
	})
	
	layer.open({
		type: 1,
		area: ['300px','200px'],
		fix: false, //不固定
		maxmin: true,
		shade:0.4,
		title: title,
		content:'<div>账号：'+admin.admName+'</div>'+
