        function fillData() {
            var self = this;

            /**
             * 获取剩余HTML
             * @param surplus 剩余数量
             * @returns {string} 生成的HTML
             */
            function getStoreHTML(surplus) {
                var html = "";
                if (isNaN(surplus) || surplus == 0) {
                    html = "售罄";
                } else if (0<surplus && surplus < 10) {
                    html = "余" + surplus
                }else if (surplus >= 10) {
                    html = "充足";
                }
                return html;
            }

            /**
             * 创建浮动框
             * @returns {*|jQuery|HTMLElement}  被创建的浮动框jQuery对象
             */
            function createHover() {
                var $hover = $(".calhover");
                if ($hover.length <= 0) {
                    $hover = $('<div class="calhover"><div class="triangle"></div></div>');
                } else {
                    $hover.html('<div class="triangle"></div>');
                }
                $("body").append($hover);
                $hover.removeClass("calhover-right");
                return $hover;
            }

