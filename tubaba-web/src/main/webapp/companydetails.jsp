                        //显示价格
                        $td.find(".calprice").html('<i>&yen;</i><em>' + price + '</em>起');
                     
                        //显示库存
                        $td.find(".calinfo").html(getStoreHTML(surplus));

                        //是否售罄
                        if (isNaN(surplus) || surplus == 0) {
